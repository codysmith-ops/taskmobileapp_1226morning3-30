# Environment Configuration Template
# Project: MobileTodoList-iOS
#
# INSTRUCTIONS:
# 1. Copy this file to .env.development (for local development)
# 2. Fill in actual values (never commit .env.development)
# 3. For production, CI/CD will provide .env.production at build time
#
# SECURITY: Never commit real API keys, tokens, or secrets to git

# API Configuration
API_BASE_URL=https://api.example.com
API_TIMEOUT=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_REMOTE_CONFIG=true

# Development Tools
DEBUG_MODE=false
ENABLE_FLIPPER=false

# Environment Identifier
ENVIRONMENT=development

# ==============================================================================
# DO NOT COMMIT REAL SECRETS BELOW THIS LINE
# ==============================================================================

# Example placeholders (replace in actual .env files)
# API_KEY=your_api_key_here
# AUTH_TOKEN=your_auth_token_here
# DATABASE_URL=your_database_url_here
