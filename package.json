{
  "name": "ellio-ios",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "lint": "eslint . --max-warnings=100",
    "start": "react-native start",
    "test": "jest",
    "pre-commit": "npm run lint",
    "xcode:audit": "python3 xcode_auditor.py --audit-only",
    "xcode:fix": "python3 xcode_auditor.py --fix",
    "xcode:fix-quick": "bash fix_build_phases.sh",
    "pods": "cd ios && pod install",
    "pods:clean": "cd ios && rm -rf Pods Podfile.lock && pod install",
    "ios:clean": "cd ios && xcodebuild clean && rm -rf build",
    "prebuild:ios": "npm run xcode:fix",
    "postinstall": "npm run pods"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/push-notification-ios": "^1.12.0",
    "@react-native-voice/voice": "^3.2.4",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "geolib": "^3.3.4",
    "nanoid": "^5.1.6",
    "react": "18.2.0",
    "react-native": "0.73.9",
    "react-native-camera": "^4.2.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-image-picker": "^8.2.1",
    "react-native-notifications": "^5.2.2",
    "react-native-permissions": "^5.4.4",
    "react-native-svg": "^15.15.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "18.20.5",
    "npm": ">=10.0.0"
  }
}
